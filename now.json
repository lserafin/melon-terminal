{
  "version": 2,
  "name": "redeem",
  "builds": [{ "src": "package.json", "use": "@now/static-build", "config": { "distDir": "build" } }],
  "routes": [
    { "src": "^/static/(.*)", "dest": "/static/$1" },
    { "src": "^/favicon.png", "dest": "/favicon.png" },
    { "src": "^/manifest.json", "dest": "/manifest.json" },
    { "src": "^/robots.txt", "dest": "/robots.txt" },
    { "src": "^/(.*)", "dest": "/index.html" }
  ]
}
